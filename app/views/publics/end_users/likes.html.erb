<div class="container px-5 px-sm-0">
  <div class="row">
    <div class="col-md-3">
      <h4>いいね一覧</h4>
        <%= render 'publics/posts/sidebar', end_user: @end_user, post: @post %>
    </div>
    <div class="col-md-8 offset-md-1">
      <table class="table table-hover">
        <thead>
          <tr>
            <th colspan="1"></th>
            <th>タイトル</th>
            <th>学習時間</th>
            <th>学習内容</th>
          </tr>
        </thead>
        <tbody>
          <% @likes_posts.each do |post| %>
            <tr>
              <td>
                <%= image_tag post.end_user.get_profile_image(50,50) %>
              </td>
              <td><%= link_to post.title, post_path(post.id) %></td>
              <td><%= post.learning_time %></td>
              <td><%= post.learning_content %></td>
            </tr>
          <% end %>
          <%#= paginate @like_posts %>
        </tbody>
      </table>
    </div>
  </div>
</div>