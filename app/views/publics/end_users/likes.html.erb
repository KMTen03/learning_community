<div class="container px-5 px-sm-0">
  <div class="row">
    <div class="col-md-2">
      
      <%= render 'publics/end_users/profile', end_user: @end_user %>
    </div>
    <div class="col-md-6 offset-md-1">
      <h4>お気に入り投稿</h4>
      <table class="table table-hover">
        <thead>
          <tr>
            <th colspan="1"></th>
            <th>タイトル</th>
            <th>学習時間</th>
            <th>学習内容</th>
          </tr>
        </thead>
        <tbody>
          <% @likes_posts.each do |post| %>
            <tr>
              <td>
                <%= image_tag post.end_user.get_profile_image(50,50) %>
              </td>
              <td><%= link_to post.title, post_path(post.id) %></td>
              <td><%= post.learning_time %></td>
              <td><%= post.learning_content %></td>
            </tr>
          <% end %>
          <%= paginate @likes_posts %>
        </tbody>
      </table>
    </div>
  </div>
</div>